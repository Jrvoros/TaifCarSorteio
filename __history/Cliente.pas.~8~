unit Cliente;

interface

type
  TCliente = class
    ID: Integer;
    Nome: string;
    CPF: string;
  end;

procedure InserirCliente(ID: Integer; Nome, CPF: string);
procedure InserirClientesFicticios;

implementation

uses
  DatabaseUtils, System.SysUtils;

procedure InserirCliente(ID: Integer; Nome, CPF: string);
var
  SQL: string;
begin
  SQL := Format(
    'INSERT INTO Clientes (ID, Nome, CPF) VALUES (%d, ''%s'', ''%s'')',
    [ID, Nome, CPF]
  );
  InserirDadosBD(SQL);
end;

procedure InserirClientesFicticios;
var
  ID: Integer;
  Nome, CPF: string;
begin
  ID := 1;
  Nome := 'Cliente A';
  CPF := '01234567890';
  InserirCliente(ID, Nome, CPF);

  ID := 2;
  Nome := 'Cliente B';
  CPF := '02345678901';
  InserirCliente(ID, Nome, CPF);

  ID := 3;
  Nome := 'Cliente C';
  CPF := '03456789012';
  InserirCliente(ID, Nome, CPF);

  ID := 4;
  Nome := 'Cliente D';
  CPF := '04567890123';
  InserirCliente(ID, Nome, CPF);

  ID := 5;
  Nome := 'Cliente E';
  CPF := '05678901234';
  InserirCliente(ID, Nome, CPF);

   // Você pode repetir o processo conforme necessário, ou receber os valores dinamicamente.
end;


end.

